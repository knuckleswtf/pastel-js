<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title><%- page.title %></title>

    <link href="https://fonts.googleapis.com/css?family=PT+Sans&display=swap" rel="stylesheet">

    <%- tools.get_css_link_tag('style', 'screen') %>
    <%- tools.get_css_link_tag('print', 'print') %>
    <%- tools.get_js_script_tag('all') %>

    <%- tools.get_css_link_tag('highlight-darcula') %>
    <%- tools.get_js_script_tag('highlight.pack') %>
    <script>hljs.initHighlightingOnLoad();</script>

</head>

<body class="" data-languages="<%= JSON.stringify(page.language_tabs || []) %>">
<a href="#" id="nav-button">
      <span>
        NAV
        <%- tools.get_image_tag('images/navbar.png') %>
      </span>
</a>
<div class="tocify-wrapper">
    <% if (page.logo != false) { %>
    <img src="<%= page.logo %>" alt="logo" class="logo" style="padding-top: 10px;" width="230px"/>
    <% } %>
    <% if (page.language_tabs.length) { %>
        <div class="lang-selector">
            <% page.language_tabs.forEach(function(lang) { %>
                <a href="#" data-language-name="<%= lang %>"><%= lang %></a>
            <% }); %>
        </div>
    <% } %>
    <div class="search">
        <input type="text" class="search" id="input-search" placeholder="Search">
    </div>
    <ul class="search-results"></ul>

    <ul id="toc">
    </ul>

    <% if (page.toc_footers.length) { %>
        <ul class="toc-footer" id="toc-footer">
            <% page.toc_footers.forEach(function(link) { %>
                <li><%- link %></li>
            <% }); %>
        </ul>
    <% } %>
        <ul class="toc-footer" id="last-updated">
            <li>Last updated: <%= page.last_updated %></li>
        </ul>
</div>
<div class="page-wrapper">
    <div class="dark-box"></div>
    <div class="content">
        <%- content %>
    </div>
    <div class="dark-box">
        <% if (page.language_tabs.length) { %>
            <div class="lang-selector">
                <% page.language_tabs.forEach(function(lang){ %>
                    <a href="#" data-language-name="<%= lang %>"><%= lang %></a>
                <% }); %>
            </div>
        <% } %>
    </div>
</div>
<% if (page.language_tabs.length) { %>
<script>
    $(function () {
        var languages = <%- JSON.stringify(page.language_tabs) %>;
        setupLanguages(languages);
    });
</script>
<% } %>
</body>
</html>